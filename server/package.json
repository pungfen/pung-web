{
  "name": "server",
  "version": "1.0.0",
  "main": "src/app.ts",
  "author": "pung",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "ts-node-dev --respawn src/app.ts",
    "dev": "nodemon --exec ts-node src/app.ts",
    "build": "tsc",
    "start:prod": "node app.js",
    "eslint-check": "npx eslint src/app.ts",
    "eslint-fix": "npx eslint src/app.ts --fix",
    "prettier-start": "npx prettier --single-quote --tab-width 2 src/app.ts",
    "prettier-clean": "npx prettier src/app.ts",
    "prettier-write": "prettierrc --write src/app.ts"
  },
  "keywords": [
    "typescript",
    "tsc",
    "javascript",
    "NodeJS"
  ],
  "dependencies": {
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^10.0.0",
    "mysql2": "^2.2.5",
    "nedb": "^1.8.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.5.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.0",
    "@types/koa": "^2.11.6",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.4.1",
    "@types/nedb": "^1.8.11",
    "@types/node": "^14.14.25",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.17.0",
    "eslint-config-koa": "^2.0.2",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "sequelize-typescript": "^2.0.0-beta.1",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  }
}
