{
  "configurations": [
    {
      "type": "edge",
      "request": "launch",
      "name": "vuejs: edge",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/client/src",
      "breakOnLoad": true,
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "启动程序",
      "program": "${workspaceFolder}/server/app.ts",
      "runtimeExecutable": "nodemon",
      "restart": true,
      "console": "integratedTerminal",
      "skipFiles": [
        "${workspaceRoot}/node_modules/**/*.js",
        "<node_internals>/**/*.js"
      ]
    }
  ]
}
